toilet -f term -F gay "THANKS FOR USING AS_01"
break
;;
0)
toilet -f term -F gay "THANKS FOR USING AS_01"
break
;;
*)
echo "INVALID NUMBER, TRY AGAIN FROM 0 TO 3 "
;;
esac
done
